{
  "manifest_version": 3,
  "name": "Bookmark Workbench",
  "version": "1.0.0",
  "description": "Chromeのブックマークをツリー形式で直接編集できる拡張機能",
  "permissions": ["bookmarks", "favicon"],
  "action": {
    "default_title": "Bookmark Workbench を開く",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
